#pnlQuerySizer
{
  bottom: auto;
  height: 250px;
  min-height: 212px;
}

#selectionDivider
{
  left: 8px;
  right: 8px;
  top: 250px;
  bottom: auto;
  height: 8px;
  cursor: n-resize;
  z-index: 3;
}

#pnlDataSizer
{
  top: 258px;
  min-height: 212px;
}

#optSelect
{
  background-position: -813px 3px;  /* Select.gif */
  behavior: url(Styles/PIE.htc);
  border-radius: 2px 2px 0px 0px;
  -webkit-border-radius: 2px 2px 0px 0px;
  -moz-border-radius: 2px 2px 0px 0px;
}

#optSelect.Disabled
{
  background-position: -837px 3px;  /* SelectDisable.gif */
}

#cmdSelectView
{
  background-position: -861px 3px;  /* SelectView.gif */
}

#cmdSelectView.Disabled
{
  background-position: -885px 3px;  /* SelectViewDisable.gif */
}

#cmdClearSelection
{
  background-position: -21px 3px;  /* Clear.gif */
  behavior: url(Styles/PIE.htc);
  border-radius: 0px 0px 2px 2px;
  -webkit-border-radius: 0px 0px 2px 2px;
  -moz-border-radius: 0px 0px 2px 2px;
}

#pnlQueryGrid
{
  left: 10px;
  top: 80px;
  right: 10px;
  bottom: 20px;
  border: solid 1px #C9C9C9;
   behavior: url(Styles/PIE.htc);
  -webkit-box-shadow: 0 1px 1px rgba(0,0,0,0.15);
  -moz-box-shadow: 0 1px 1px rgba(0,0,0,0.15);
  box-shadow: 0 1px 1px rgba(0,0,0,0.15);
  background-color: #F0F0F0;
  overflow: auto;
}

#pnlQueryCommand
{
  left: 10px;
  bottom: 6px;
  top: auto;
  height: 11px;
}

#pnlDataTabs
{
  left: 12px;
  right: 12px;
  bottom: auto;
  height: 19px;
  overflow: hidden;
}

#pnlData
{
  top: 18px;
}

#pnlDataList
{
  left: 10px;
  top: 17px;
  right: 10px;
  bottom: 10px;
  border: solid 1px #C9C9C9;
   behavior: url(Styles/PIE.htc);
  -webkit-box-shadow: 0 1px 1px rgba(0,0,0,0.15);
  -moz-box-shadow: 0 1px 1px rgba(0,0,0,0.15);
  box-shadow: 0 1px 1px rgba(0,0,0,0.15);
  background-color: White;
  overflow: auto;
}

#pnlDataList .DataList
{
  left: 10px;
  top: 10px;
  right: 30px;
}

#cmdDataPrint
{
  left: auto;
  bottom: auto;
  right: 12px;
  top: 2px;
  text-align: right;
}